    "google-closure-library": "^20201102.0.1",
    "jasmine": "^2.9.0",
    "jasmine-diff": "^0.1.3"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "minimatch": "^3.0.4",
    "yargs": "^12.0.2"
  }
